extends layout
block content
    div(class="container")
        div(class="col")
        div(class="col")
            div(class="border" style="padding: 10px; margin-top: 20px; margin-buttom: 20px;")
              if scripts.length > 0
                each value,index in scripts
                    h6 #{scripts[index].title}
                    p #{scripts[index].content}
                    if index < scripts.length - 1
                        hr
        div(class="col")
